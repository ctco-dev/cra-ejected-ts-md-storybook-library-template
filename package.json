{
  "name": "cra-ejected-ts-storybook",
  "version": "1.0.7",
  "private": true,
  "dependencies": {
    "d3": "5.5.0",
    "material-ui": "0.20.1",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-tap-event-plugin": "3.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-centered": "3.4.8",
    "@storybook/addon-storyshots": "3.4.8",
    "@storybook/react": "3.4.8",
    "@types/jest": "23.1.6",
    "@types/material-ui": "0.21.3",
    "@types/node": "10.5.2",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/storybook__react": "3.0.8",
    "autoprefixer": "8.6.5",
    "awesome-typescript-loader": "5.2.0",
    "babel-preset-react-app": "3.1.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.1",
    "react-dev-utils": "5.0.1",
    "react-error-overlay": "4.0.0",
    "react-test-renderer": "16.4.1",
    "style-loader": "0.21.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "ts-jest": "23.0.0",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.16.1",
    "webpack-dev-server": "3.1.4",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build-libs": "node scripts/build-libs.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{tsx}",
      "!src/**/*stories.{tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^(?!.*\\.(ts|tsx|js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx|js|jsx)$"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testURL": "http://localhost"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
